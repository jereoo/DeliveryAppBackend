{
  "expo": {
    "name": "DeliveryAppMobile",
    "slug": "deliveryappmobile",
    "version": "1.2.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
