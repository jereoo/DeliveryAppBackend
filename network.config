[profile]
active = phone

[backend.private]
host = 127.0.0.1
port = 8000
debug = True
allowed_hosts = localhost,127.0.0.1
cors_origins = http://localhost:3000

[backend.public]
host = 0.0.0.0
port = 8080
debug = True
allowed_hosts = localhost,127.0.0.1,0.0.0.0
cors_origins = http://localhost:3001,http://0.0.0.0:3001

[backend.hotspot]
host = 0.0.0.0
port = 8000
debug = True
allowed_hosts = localhost,127.0.0.1,0.0.0.0,192.168.1.82
cors_origins = http://localhost:3000,http://0.0.0.0:3000,http://192.168.1.82:3000

[frontend.private]
port = 3000
api_base_url = http://127.0.0.1:8000
proxy_target = http://127.0.0.1:8000

[frontend.public]
port = 3001
api_base_url = http://localhost:8080
proxy_target = http://localhost:8080

[frontend.hotspot]
port = 3000
api_base_url = http://0.0.0.0:8000
proxy_target = http://0.0.0.0:8000

[backend.phone]
host = 0.0.0.0
port = 8081
debug = True
allowed_hosts = localhost,127.0.0.1,0.0.0.0,192.168.1.87,172.20.10.6
cors_origins = http://localhost:3000,http://0.0.0.0:3000,http://192.168.1.87:3000,http://192.168.1.87:8081,http://172.20.10.6:8081

[frontend.phone]
port = 3000
api_base_url = http://192.168.1.87:8081
proxy_target = http://192.168.1.87:8081

[database]
engine = django.db.backends.postgresql
name = delivery_app
user = delivery_user
host = localhost
port = 5432

[jwt]
access_token_lifetime_minutes = 15
refresh_token_lifetime_days = 7
rotate_refresh_tokens = True
blacklist_after_rotation = True

